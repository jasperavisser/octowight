<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
	<appender name="console" class="ch.qos.logback.core.ConsoleAppender">
		<target>System.out</target>
		<encoder>
			<pattern>%d{MMM dd HH:mm:ss,SSS} %c: %m%n</pattern>
		</encoder>
	</appender>
	<root level="info">
		<appender-ref ref="console"/>
	</root>
</configuration>

